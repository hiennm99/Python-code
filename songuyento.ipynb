    "def snt(x):\n",
    "    i=0\n",
    "    d=0\n",
    "    while i<=x:\n",
    "        i+=1\n",
    "        if (x%i==0):\n",
    "            d+=1\n",
    "        continue\n",
    "\n",
    "    if (d==2):\n",
    "        print(x,' la so nguyen to')\n",
    "    else:\n",
    "        print(x,' khong la so nguyen to')"
